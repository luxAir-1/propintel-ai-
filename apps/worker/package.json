{
  "name": "@propintel/worker",
  "version": "0.1.0",
  "description": "PropIntel AI - BullMQ Workers (Scoring, PDF, Alerts)",
  "author": "PropIntel Team",
  "private": true,
  "license": "PROPRIETARY",
  "scripts": {
    "build": "tsc && tsc-alias -p tsconfig.json",
    "start": "node dist/main.js",
    "start:dev": "ts-node src/main.ts",
    "start:scoring": "ts-node src/workers/scoring.worker.ts",
    "start:pdf": "ts-node src/workers/pdf.worker.ts",
    "start:alerts": "ts-node src/workers/alerts.worker.ts",
    "lint": "eslint \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "bullmq": "^4.13.0",
    "dotenv": "^16.3.1",
    "puppeteer": "^21.6.0",
    "redis": "^4.6.12",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "anthropic": "^0.13.0",
    "resend": "^2.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.3.3"
  }
}
